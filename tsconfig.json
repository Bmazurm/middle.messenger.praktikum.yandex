{
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": "./src/",
    "target": "es2020",
    "declaration": false,
    "module": "ES6",
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "lib": ["es2015", "es2017.object", "dom"]
  }
}
